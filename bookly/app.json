{
  "expo": {
    "name": "Bookly",
    "slug": "bookly",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo_bookly.png",
    "scheme": "bookly",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#181B20",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.gabmello.bookly"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/logo_bookly.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo_bookly.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#181B20",
          "dark": {
            "backgroundColor": "#181B20"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3eb08233-5c6f-4ac1-b441-80d7a68dba5f"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/3eb08233-5c6f-4ac1-b441-80d7a68dba5f"
    }
  }
}
